    "del-cli": "^2.0.0",
    "eslint": "^6.0.1",
    "eslint-loader": "^2.1.2",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.3",
    "mini-css-extract-plugin": "^0.7.0",
    "ncp": "^2.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
