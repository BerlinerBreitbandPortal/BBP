{
  "name": "breitband-bedarfsplattform",
  "version": "1.6.0",
  "description": "A platform to visualize broadband needs in Berlin",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --config webpack_config/webpack.dev.config.js --mode development",
    "build": "webpack --config webpack_config/webpack.prod.config.js --mode production",
    "build-login": "del-cli ./dist-login && ncp ./src/login ./dist-login && replace-in-file /breitbandbedarfsplattform/dev/login/index.php /login/index.php ./dist-login/users/init.php",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "create-jsdoc": "del-cli ./documentation && jsdoc -c ./jsdoc-conf.json && cpy ./resources/jsdoc_readme/Berliner_Breitband_Portal.png documentation"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BerlinerBreitbandPortal/BBP.git"
  },
