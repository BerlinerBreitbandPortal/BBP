  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootstrap-select": "^1.13.10",
    "core-js": "^3.2.1",
    "datatables.net": "^1.10.19",
    "datatables.net-buttons": "^1.6.1",
    "datatables.net-buttons-dt": "^1.6.1",
    "datatables.net-dt": "^1.10.19",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "moment": "^2.24.0",
    "ol": "^5.3.3",
    "ol-layerswitcher": "^3.3.0",
    "popper.js": "^1.15.0"
  },
